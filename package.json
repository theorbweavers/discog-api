{
  "name": "discog-api",
  "version": "1.0.0",
  "description": "API to manage discography for bands",
  "main": "src/server.js",
  "config": {
    "server": "bin/server",
    "proxy": "index",
    "region": "ap-southeast-2",
    "timeout": 30,
    "memory": 128
  },
  "files": [
    "bin/",
    "index.js"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development babel-node src/server.js",
    "transpile": "NODE_ENV=production babel src --out-dir bin",
    "generate-proxy": "claudia generate-serverless-express-proxy --express-module $npm_package_config_server --proxy-module-name $npm_package_config_proxy",
    "create:lambda": "claudia create --handler $npm_package_config_proxy.handler --deploy-proxy-api --timeout $npm_package_config_timeout --memory $npm_package_config_memory --region $npm_package_config_region --set-env-from-json env.prod.json",
    "deploy:lambda": "claudia update",
    "create": "npm run transpile && npm run generate-proxy && npm run create:lambda",
    "deploy": "npm run transpile && npm run deploy:lambda",
    "destroy": "claudia destroy"
  },
  "author": "Stuart Flanagan <stuartflanagan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "aws-serverless-express": "^1.3.0",
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "jwks-rsa": "^1.1.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^4.9.1",
    "morgan": "^1.8.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theorbweavers/discog-api.git"
  },
  "keywords": [
    "Music",
    "API",
    "serverless",
    "aws"
  ],
  "bugs": {
    "url": "https://github.com/theorbweavers/discog-api/issues"
  },
  "homepage": "https://github.com/theorbweavers/discog-api#readme"
}
